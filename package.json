{
  "name": "webgis",
  "version": "1.0.0",
  "description": "A website that displays remote map datasets using leafletjs",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo 'tests are in the browser'",
    "build": "babel js_src -d static/js",
    "dev": "babel js_src --watch -d static/js",
    "dev-portal": "watchify js_src/new/portal.js -o static/js/new/portal.js -v -d -t [ babelify ]"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patcurry/WebGIS.git"
  },
  "keywords": [
    "leaflet"
  ],
  "author": "Patrick Curry",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "watch": "^1.0.2",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "@turf/turf": "^4.4.0",
    "easy-nominatim": "^1.0.4",
    "file-saver": "^1.3.3",
    "leaflet": "^1.0.3",
    "leaflet.markercluster": "^1.0.5",
    "requirejs": "^2.3.2"
  },
  "false": {}
}
