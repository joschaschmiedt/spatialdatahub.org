{
  "name": "webgis",
  "version": "1.0.0",
  "description": "A website that displays remote map datasets using leafletjs",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "browserify js_src/new/portal.js -t babelify -o static/js/new/portal.js",
    "dev": "watchify js_src/new/portal.js -t babelify -o static/js/new/portal.js",
    "test": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patcurry/WebGIS.git"
  },
  "keywords": [
    "leaflet"
  ],
  "author": "Patrick Curry",
  "license": "MIT",
  "devDependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha": "^1.3.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^3.4.2",
    "timers-browserify": "^2.0.2",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "@turf/turf": "^4.4.0",
    "easy-nominatim": "^1.0.4",
    "file-saver": "^1.3.3",
    "leaflet": "^1.0.3",
    "leaflet.markercluster": "^1.0.5",
    "requirejs": "^2.3.2"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "false": {}
