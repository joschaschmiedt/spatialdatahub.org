{
  "name": "webgis",
  "version": "1.0.0",
  "description": "A website that displays remote map datasets using leafletjs",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "browserify js_src/bundle.js -t babelify -o static/js/bundle.js -v",
    "dev": "watchify js_src/bundle.js -t babelify -o static/js/bundle.js -v",
    "devserial": "watchify js_src/serialized.js -t babelify -o static/js/serialized.js -v",
    "devreact": "watchify js_src/reactify.js -t babelify -o static/js/reactify.js -v",
    "test": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patcurry/WebGIS.git"
  },
  "keywords": [
    "leaflet"
  ],
  "author": "Patrick Curry",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.3.1",
    "jest": "^22.3.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.3.1",
    "mocha": "^3.4.2",
    "react-test-renderer": "^16.2.0",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0",
    "timers-browserify": "^2.0.2",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "@turf/turf": "^4.5.2",
    "@turf/within": "^4.5.2",
    "axios": "^0.17.1",
    "easy-nominatim": "^1.0.4",
    "file-saver": "^1.3.3",
    "leaflet": "^1.1.0",
    "leaflet.markercluster": "^1.0.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "regenerator": "^0.12.3",
    "requirejs": "^2.3.2",
    "rxjs": "^5.5.6",
    "semver": "^5.3.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "false": {}
}
